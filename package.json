        "concurrently": "^5.3.0",
        "electron": "^11.1.1",
        "electron-builder": "^22.9.1",
        "eslint-config-prettier": "^7.1.0",
        "node-sass": "^4.14.1",
        "prettier": "^2.2.1",
        "react-router-dom": "^5.2.0",
        "stylelint": "^13.8.0",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-scss": "^3.18.0",
        "wait-on": "^5.2.1"
